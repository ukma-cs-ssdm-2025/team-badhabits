{
  "name": "wellity-backend",
  "version": "1.0.0",
  "description": "Wellity Custom Backend - REST API for adaptive workouts, payments, and analytics",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "audit": "npm audit",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:verbose": "jest --verbose",
    "generate-docs": "node scripts/generate-openapi.js",
    "generate-openapi": "node scripts/generate-openapi.js",
    "docs:serve": "redoc-cli serve ../../docs/api/openapi-generated.yaml -p 8080",
    "docs:build": "redoc-cli bundle ../../docs/api/openapi-generated.yaml -o ../../docs/api/index.html"
  },
  "keywords": [
    "wellity",
    "fitness",
    "rest-api"
  ],
  "author": "Team BadHabits",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "js-yaml": "^4.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@apidevtools/swagger-cli": "^4.0.4",
    "@eslint/js": "^9.38.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "redoc-cli": "^0.13.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
